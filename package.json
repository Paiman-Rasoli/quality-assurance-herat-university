{
  "dependencies": {
    "concurrently": "^7.4.0",
    "cors": "^2.8.5"
  },
  "scripts": {
    "server": "cd qa-backend & yarn run dev",
    "client": "cd qa-frontend & yarn start",
    "start": "concurrently --kill-others \"yarn run server\" \"yarn run client\"",
    "installserver": "cd qa-backend & yarn",
    "installclient": "cd qa-frontend & yarn",
    "install-all": "yarn add concurrently && yarn run installserver && yarn run installclient"
  }
}
